<?xml version="1.0" encoding="UTF-8"?>
 
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
	
	<changeSet author="martingielow" id="jsslab_data_disable-foreign-key-checks-mysql" runAlways="true" dbms="mysql">
		<comment>Disables foreign key checks to enable batch insertion of data - MySQL</comment>
		<sql>SET FOREIGN_KEY_CHECKS=0;</sql>
	</changeSet>
	
	<!-- Adds sample data to the regular OpenMRS tables -->
	<include file="liquibase-sample-data-regular.xml" relativeToChangelogFile="false" />
	
	<!-- Adds sample data to the JSSLab tables -->
	<include file="liquibase-sample-data-jsslab.xml" relativeToChangelogFile="false" />

	<changeSet author="martingielow" id="jsslab_data_enable-foreign-key-checks-mysql" runAlways="true" dbms="mysql">
		<comment>Reenables foreign key checks after batch insertion - MySQL</comment>
		<sql>SET FOREIGN_KEY_CHECKS=1;</sql>
	</changeSet>
		
</databaseChangeLog>
<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">
	
    <!-- Module Properties -->
	<id>jsslab</id>
	<name>JSSLab Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.jsslab</package>
	<author>rfriedman</author>
	<description>
			Specimen-based lab module for JSS
	</description>
	
	<!-- / Module Properties -->
	
	<require_version>1.2.0.3400</require_version>


    <!-- Module Activator -->
	<activator>org.openmrs.module.jsslab.jsslabActivator</activator>


	
		<!-- Add's link to admin page --> 
	 <extension>
	  <point>org.openmrs.admin.list</point>
	  <class>org.openmrs.module.jsslab.extension.html.AdminList</class>
	 </extension>    
  	  	
  	<!-- Maps hibernate file's , if present -->   	
   	<mappingFiles>
		${omodHbmConfig}
	</mappingFiles>

	
	<!-- Internationalization -->
	<!-- All message codes should start with @MODULE_ID@.* -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->
	
</module>

